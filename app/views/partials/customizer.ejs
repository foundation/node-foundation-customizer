<li><label><input type="checkbox" name="scss_components[]" value="foundation/components/reveal" data-js-dependency="foundation/foundation.reveal.js" checked="checked"> Reveal</label></li>
<li><label><input type="checkbox" name="scss_components[]" value="foundation/components/range-slider" data-js-dependency="foundation/foundation.slider.js" checked="checked"> Slider</label></li>
<li><label><input type="checkbox" name="scss_components[]" value="foundation/components/tabs" data-js-dependency="foundation/foundation.tab.js" checked="checked"> Tabs</label></li>
<li><label><input type="checkbox" name="scss_components[]" value="foundation/components/tooltips" data-js-dependency="foundation/foundation.tooltip.js" checked="checked"> Tooltips</label></li>
<li><label><input type="checkbox" name="scss_components[]" value="foundation/components/top-bar" checked="checked" data-scss-dependency="foundation/components/grid" data-js-dependency="foundation/foundation.topbar.js"> Top Bar</label></li>
</ul>
</div>
</div>
</div>

