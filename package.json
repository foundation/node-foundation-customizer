{
  "name": "node-foundation-customizer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "gulp",
    "postinstall": "bower install",
    "foundation": "npm run foundation:clone && npm run foundation:checkout && npm run foundation:install",
    "foundation:clone": "git clone git@github.com:foundation/foundation-sites && npm run foundation:checkout",
    "foundation:checkout": "cd foundation-sites && git fetch && git checkout v6.5.3 && cd ..",
    "foundation:install": "cd foundation-sites && npm install && cd ..",
    "foundation:update": "cd foundation-sites && git pull && npm install && cd .."
  },
  "dependencies": {
    "archiver": "^0.16.0",
    "bluebird": "^3.0.5",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.4.2",
    "gulp-uncss": "^1.0.4",
    "md5": "^2.1.0",
    "morgan": "~1.6.1",
    "node-sass": "^4.5.3",
    "open": "0.0.5",
    "rimraf": "^2.4.3",
    "serve-favicon": "~2.3.0"
  }
}
